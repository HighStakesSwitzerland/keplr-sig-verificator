  "devDependencies": {
    "@keplr-wallet/cosmos": "^0.10.16",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.224",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.5",
    "@cosmjs/amino": "^0.28.11",
    "@keplr-wallet/crypto": "^0.10.16",
    "@keplr-wallet/types": "^0.9.0-alpha.14",
    "jscrypto": "^1.0.0",
